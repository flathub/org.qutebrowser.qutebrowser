{
    "name": "zsh",
    "config-opts": [
        "--enable-etcdir=/app/etc/zsh",
        "--enable-fndir=/app/share/zsh/functions",
        "--enable-function-subdirs",
        "--enable-multibyte",
        "--enable-pcre",
        "--enable-scriptdir=/app/share/zsh/scripts",
        "--enable-zlogin=/app/etc/zsh/zlogin",
        "--enable-zlogout=/app/etc/zsh/zlogout",
        "--enable-zprofile=/app/etc/zsh/zprofile",
        "--enable-zsh-secure-free",
        "--enable-zshenv=/app/etc/zsh/zshenv",
        "--enable-zshrc=/app/etc/zsh/zshrc",
        "--with-tcsetpgrp",
        "--with-term-lib=ncursesw"
    ],
    "sources": [
        {
            "type": "archive",
            "url": "https://www.zsh.org/pub/zsh-5.8.1.tar.xz",
            "sha256": "b6973520bace600b4779200269b1e5d79e5f505ac4952058c11ad5bbf0dd9919",
            "x-checker-data": {
                "type": "anitya",
                "project-id": 5307,
                "stable-only": true,
                "url-template": "https://www.zsh.org/pub/zsh-$version.tar.xz"
            }
        }
    ],
    "cleanup": [
        "/share/man"
    ],
    "modules": [
        "ncurses/libncurses.json"
    ]
}
